<<addclue "Game Manager" "Crumpled Bedsheets" "The bedsheets are crumpled, as if they haven’t been touched since this morning." "Sate">>

<<if $clueCount == 2>>
    <<jump ExplorationDone>>
<<endif>>

<<enddialogue "Game Manager">>
